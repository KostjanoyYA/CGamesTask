<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <target>System.out</target>
        <encoder>
            <pattern>%date{HH:mm:ss.SSS} %class %thread %message%n</pattern>
        </encoder>
    </appender>
    <root level="trace">
        <appender-ref ref="console"/>
    </root>
</configuration>